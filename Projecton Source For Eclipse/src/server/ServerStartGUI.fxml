<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="server.ServerStartGui">
   <center>
      <AnchorPane prefHeight="196.0" prefWidth="408.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="196.0" fitWidth="408.0" pickOnBounds="true">
               <image>
                  <Image url="@../resources/background_image.jpg" />
               </image>
            </ImageView>
            <Label layoutX="134.0" layoutY="18.0" prefHeight="48.0" prefWidth="139.0" text="Server Options">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
            </Label>
            <Button fx:id="ConnectButton" layoutX="82.0" layoutY="111.0" mnemonicParsing="false" onAction="#connectServer" prefHeight="28.0" prefWidth="92.0" style="-fx-background-radius: 10;" text="Connect" />
            <Button fx:id="DissconnectButton" layoutX="252.0" layoutY="109.0" mnemonicParsing="false" onAction="#DissconnectServer" prefHeight="32.0" prefWidth="82.0" style="-fx-background-radius: 10;" text="Disconnect" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
